require(survival)
# load data
data("simu_data")
# create a formula using a Surv
formula_aft <- Surv(y_L, y_R, type = "interval2") ~ X1 + X2 - 1
# fit a model
aft_fit <- aftsur(formula = formula_aft, cure_var = ~ Z1 + Z2 + Z3, offset = TRUE, data = simu_data)
predict(aft_fit,x = c(-2,-5), type = "hazard", interval = T)
predict(aft_fit,time = c(1), type = "hazard", interval = T)
predict(aft_fit,time = c(1), type = "survival", interval = T)
plot_sur(aft_fit, x = c(1,-10))
plot_sur(aft_fit, x = c(1,-2))
